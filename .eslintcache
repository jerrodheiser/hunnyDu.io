Count":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"126","messages":"127","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"128","messages":"129","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"64"},{"filePath":"130","messages":"131","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/reportWebVitals.js",[],["132","133"],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/index.js",[],["134","135"],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/TaskCard.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/Header.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/Context/index.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/App.js",[],["136","137"],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/NewTaskForm.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/Dashboard.js",["138"],"import React, {useContext, useEffect, useState} from 'react';\r\nimport TaskCard from './TaskCard';\r\nimport {HunnyduContext} from './Context/index';\r\nimport Container from 'react-bootstrap/Container';\r\nimport Row from 'react-bootstrap/Row';\r\nimport Col from 'react-bootstrap/Col';\r\nimport Spinner from 'react-bootstrap/Spinner';\r\nimport Jumbotron from 'react-bootstrap/Jumbotron';\r\nimport Accordion from 'react-bootstrap/Accordion';\r\nimport Button from 'react-bootstrap/Button';\r\n\r\n\r\n// Component that displays a user's/family's tasks.\r\nconst Dashboard = () => {\r\n\r\n  // Declare context api data.\r\n  const {tasks, actions, familyName, familyTasks, isLeader} = useContext(HunnyduContext);\r\n\r\n  // Declare state variable for which tasks are displayed (individual's vice\r\n  //  entire family's).\r\n  const [displayAll, setDisplayAll] = useState(isLeader ? true : false);\r\n\r\n  // Declare state variable for status of loading data.\r\n  const [isLoading, setIsLoading] = useState(true);\r\n\r\n\r\n  // Refresh all tasks and clear loading state on initial render.\r\n  useEffect (() => {\r\n    actions.refreshTasks()\r\n      .then(res => {\r\n        setIsLoading(false);\r\n      });\r\n  },[]);\r\n\r\n\r\n  // Return component jsx.\r\n  return (\r\n    <Container>\r\n      {familyName\r\n        ?\r\n          <>\r\n          <h1 className=\"pageTitle\" >\r\n            {familyName} - {displayAll\r\n                            ? 'Family Dashboard'\r\n                            : 'Your Dashboard'}\r\n          </h1>\r\n          { // Renders either a loading display, or tasks if loading is complete.\r\n            isLoading\r\n              ?\r\n                <Col lg className=\"loading-visual\">\r\n                  <Row>\r\n                    <Spinner animation=\"grow\" role=\"status\">\r\n                      <span className=\"sr-only\">Loading...</span>\r\n                    </Spinner>\r\n                  </Row>\r\n                </Col>\r\n              :\r\n                <>\r\n                  {// Renders tasks is present, otherwise a \"no tasks\" message.\r\n                    tasks[0] || familyTasks[0]\r\n                    ?\r\n                      <>\r\n                        <Button variant=\"primary\" onClick={()=>setDisplayAll(!displayAll)}>\r\n                          {displayAll\r\n                            ? 'Show Your Tasks'\r\n                            : 'Show Family\\'s Tasks'\r\n                          }\r\n                        </Button>\r\n                        <Accordion>\r\n                          {/*\r\n                            Bootstrap's event_index cannot be 0.\r\n                          */}\r\n                          {displayAll\r\n                            ? familyTasks.map( (task, index) =>\r\n                                <TaskCard\r\n                                  task={task}\r\n                                  event_index={index+1}\r\n                                  key={index.toString()}\r\n                                  displayingAll={displayAll}\r\n                                />\r\n                              )\r\n                            : tasks.map( (task, index) =>\r\n                                <TaskCard\r\n                                  task={task}\r\n                                  event_index={index+1}\r\n                                  key={index.toString()}/>\r\n                              )\r\n                          }\r\n                        </Accordion>\r\n                      </>\r\n                    :\r\n                      <p>No tasks to display.</p>\r\n                  }\r\n                </>\r\n          }\r\n        </>\r\n      :\r\n        <>\r\n          <Jumbotron>\r\n            <h1>You are not currently part of a family.</h1>\r\n            <p>\r\n              Other family leaders can let you into their family, or you can\r\n              <a href='/newFamily'> click here </a> to start your own.\r\n            </p>\r\n          </Jumbotron>\r\n        </>\r\n      }\r\n    </Container>\r\n  );\r\n}\r\n\r\n\r\nexport default Dashboard;\r\n","/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/NotFound.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/SubtaskCard.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/NewSubtaskForm.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/Data/index.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/LoginForm.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/PrivateRoute.js",["139"],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/LeaderRoute.js",["140"],"import React from 'react';\nimport { Route, Redirect } from 'react-router-dom';\nimport { Consumer } from './Context';\n\n\n// Route component that prevents rendering routes restricted to some users.\nexport default ({ component: Component, ...rest }) => {\n  return (\n    <Consumer>\n      { context => (\n        <Route\n          {...rest}\n          render={props => context.isAuthenticated && context.isLeader ? (\n              <Component {...props} />\n            ) : (\n              <Redirect to={{\n                pathname:'/'\n              }} />\n            )\n          }\n        />\n      )}\n    </Consumer>\n  );\n};\n",["141","142"],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/RegistrationForm.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/ConfirmUser.js",[],["143","144"],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/RequestPasswordResetForm.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/PasswordResetForm.js",["145"],"import React, { useRef, useState, useContext, useEffect } from 'react';\r\nimport { HunnyduContext } from './Context/index';\r\nimport Form from 'react-bootstrap/Form';\r\nimport Col from 'react-bootstrap/Col';\r\nimport Row from 'react-bootstrap/Row'\r\nimport Container from 'react-bootstrap/Container';\r\nimport Button from 'react-bootstrap/Button';\r\nimport Spinner from 'react-bootstrap/Spinner';\r\n\r\n\r\n// Form used to reset a user's password if forgotten.\r\nconst PasswordResetForm = (props) => {\r\n\r\n  // Declare context api data.\r\n  const {actions} = useContext(HunnyduContext);\r\n\r\n  // Declare state variables for handling form validation.\r\n  const [hasBeenValidated, setHasBeenValidated] = useState(false);\r\n  const [pwordMatch, setPwordMatch] = useState(true);\r\n\r\n  // Declare the token which was provided in the password reset email.\r\n  const {match} = props\r\n  const token = match.params.token;\r\n\r\n  // Declare state variable\r\n  const [isLoading, setIsLoading] = useState(true)\r\n\r\n  // Creates refs for the other form fields.\r\n  const password1 = useRef();\r\n  const password2 = useRef()\r\n\r\n\r\n  // Checks token validity on initial component render, and redirects if the\r\n  //  token is invalid.\r\n  useEffect (() => {\r\n    actions.validateResetRequest(token)\r\n      .then(res => {\r\n        if (res) {\r\n          if (res.status === 200) {\r\n            // Returned response is code 200 for valid token.\r\n            console.log('Successful confirmation!');\r\n            setIsLoading(false);\r\n          }\r\n        }\r\n        else {\r\n          // Returned response is code 401 for invalid token.\r\n          console.log('Invalid token.');\r\n          props.history.push('/');\r\n        }\r\n      })\r\n  },[]);\r\n\r\n\r\n  // Method to handle form submission.\r\n  const handleSubmit = (e) => {\r\n    e.preventDefault();\r\n    let form = e.currentTarget;\r\n    // Validate that required fields contain data.\r\n    if (form.checkValidity() === false) {\r\n      setHasBeenValidated(true);\r\n      e.stopPropagation();\r\n    } else {\r\n      // Custom validation.\r\n      if (password1.current.value !== password2.current.value) {\r\n        setPwordMatch(false);\r\n        setHasBeenValidated(true);\r\n        password1.current.value = '';\r\n        password2.current.value = '';\r\n        e.stopPropagation();\r\n      }\r\n      else {\r\n        // No response data sent back to the user.\r\n        actions.resetPassword(password1.current.value, token)\r\n          .then(res => {\r\n            if (res) {\r\n              if (res.status === 200) {\r\n                // Returned response is code 200 for successful change.\r\n                console.log('Password updated.');\r\n              }\r\n            }\r\n            else {\r\n              // Returned response is code 401 for invalid token.\r\n              console.log('Invalid token.');\r\n            }\r\n            props.history.push('/');\r\n          })\r\n      }\r\n    }\r\n  }\r\n\r\n\r\n  // Return component jsx.\r\n  return (\r\n    !isLoading\r\n      ?\r\n        <Container >\r\n          <h1 className=\"pageTitle\"> Reset password: </h1>\r\n          <Form\r\n          noValidate\r\n          validated={hasBeenValidated}\r\n          className=\"subtaskForm\"\r\n          onSubmit={handleSubmit}>\r\n            <Form.Group as={Row}>\r\n              <Col>\r\n                <Form.Control\r\n                required\r\n                type=\"password\"\r\n                placeholder=\"Password\"\r\n                ref={password1} />\r\n                <Form.Control.Feedback type=\"invalid\">\r\n                  {pwordMatch\r\n                    ? 'Please provide a password.'\r\n                    : 'Passwords do not match.'\r\n                  }\r\n                </Form.Control.Feedback>\r\n              </Col>\r\n            </Form.Group>\r\n            <Form.Group as={Row}>\r\n              <Col>\r\n                <Form.Control\r\n                required\r\n                type=\"password\"\r\n                placeholder=\"Repeat password\"\r\n                ref={password2} />\r\n                <Form.Control.Feedback type=\"invalid\">\r\n                  {pwordMatch\r\n                    ? 'Please provide a password.'\r\n                    : 'Passwords do not match.'\r\n                  }</Form.Control.Feedback>\r\n              </Col>\r\n            </Form.Group>\r\n\r\n            <Row>\r\n              <Button variant=\"success\" size=\"md\" type={\"submit\"}>Reset Password</Button>\r\n            </Row>\r\n          </Form>\r\n          <br />\r\n        </Container>\r\n      :\r\n        <Col lg className=\"loading-visual\">\r\n          <Row>\r\n            <Spinner animation=\"grow\" role=\"status\">\r\n              <span className=\"sr-only\">Loading...</span>\r\n            </Spinner>\r\n          </Row>\r\n        </Col>\r\n  );\r\n}\r\n\r\n\r\nexport default PasswordResetForm;\r\n","/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/ViewFamily.js",["146"],"import React, {useContext, useEffect} from 'react';\r\nimport {HunnyduContext} from './Context/index';\r\nimport Container from 'react-bootstrap/Container';\r\nimport Card from 'react-bootstrap/Card';\r\n\r\n\r\n// Component that displays a user's family.\r\nconst ViewFamily = () => {\r\n\r\n  // Declare context api data.\r\n  const {members, actions, familyName} = useContext(HunnyduContext);\r\n\r\n\r\n  // Refreshes family information on initial render.\r\n  useEffect (() => {\r\n    actions.refreshFamily();\r\n  },[]);\r\n\r\n\r\n  // Return component jsx.\r\n  return (\r\n    <Container>\r\n      <h1 className=\"pageTitle\" >\r\n        {familyName} - Family Roster\r\n      </h1>\r\n      {members.map( (member, index) =>\r\n        <Container key={index.toString()}>\r\n          <Card bg='light'>\r\n            <Card.Body>\r\n              <a href={`/viewProfile/${member.id}`}>\r\n                {member.name} {member.isLeader ? '(leader)' : ''}</a>\r\n            </Card.Body>\r\n          </Card>\r\n        </Container>\r\n      )}\r\n    </Container>\r\n  );\r\n}\r\n\r\n\r\nexport default ViewFamily;\r\n","/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/ManageFamily.js",["147"],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/ViewProfile.js",["148"],"import React, {useContext, useEffect, useState} from 'react';\r\nimport {HunnyduContext} from './Context/index';\r\nimport Container from 'react-bootstrap/Container';\r\nimport Row from 'react-bootstrap/Row';\r\nimport Col from 'react-bootstrap/Col';\r\nimport Spinner from 'react-bootstrap/Spinner';\r\nimport ListGroup from 'react-bootstrap/ListGroup';\r\n\r\n\r\n// Component that displays a user's profile.\r\nconst ViewProfile = (props) => {\r\n\r\n  // Declare context api data.\r\n  const {actions, id} = useContext(HunnyduContext);\r\n  const {match} = props;\r\n  const userId = parseInt(match.params.id);\r\n\r\n  // Declare state variable for status of loading data.\r\n  const [isLoading, setIsLoading] = useState(true);\r\n\r\n  // Declare state variable for user.\r\n  const [user, setUser] = useState();\r\n\r\n  // Retrieve user information when userId is changed.\r\n  useEffect (() => {\r\n    actions.getProfile(userId)\r\n      .then( res => {\r\n        if (res) {\r\n          // Returned response is code 200 for successful retrieval.\r\n          if (res.status === 200) {\r\n            setUser(res.data);\r\n            setIsLoading(false);\r\n          }\r\n        } else {\r\n          // Returned response is code 404 for user not found.\r\n          console.log('User retrieval failed.')\r\n          props.history.push('/');\r\n        }\r\n      });\r\n  },[userId]);\r\n\r\n\r\n  // Return component jsx.\r\n  return (\r\n    <Container>\r\n      {isLoading\r\n        ?\r\n          <Col lg className=\"loading-visual\">\r\n            <Row>\r\n              <Spinner animation=\"grow\" role=\"status\">\r\n                <span className=\"sr-only\">Loading...</span>\r\n              </Spinner>\r\n            </Row>\r\n          </Col>\r\n        :\r\n          <>\r\n            <h1 className=\"pageTitle\">{user.username}'s Profile</h1>\r\n            <ListGroup variant='flush'>\r\n              {userId === id\r\n                ?\r\n                  <>\r\n                    <ListGroup.Item>{user.email} - <a href='/changeEmail'>\r\n                        Change</a>\r\n                    </ListGroup.Item>\r\n                    <ListGroup.Item><a href='/changePassword'>\r\n                        Change password</a>\r\n                    </ListGroup.Item>\r\n                  </>\r\n                :\r\n                  ''\r\n              }\r\n            </ListGroup>\r\n            <br/>\r\n            <h2> Tasks </h2>\r\n            <ListGroup>\r\n              {user.tasks.map((task, index) =>\r\n                <ListGroup.Item key={index.toString()}>\r\n                  {task.taskname}\r\n                </ListGroup.Item>\r\n              )}\r\n            </ListGroup>\r\n          </>\r\n      }\r\n    </Container>\r\n  );\r\n}\r\n\r\n\r\nexport default ViewProfile;\r\n","/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/ChangeEmailForm.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/ConfirmEmailChange.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/NewFamilyForm.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/ChangePasswordForm.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/ConfirmInviteToken.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/FamilyInviteForm.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/UnconfirmedFirst.js",[],"/mnt/c/Users/jheis/documents/hunnydu/hunnydu/src/components/UnconfirmedAgain.js",[],{"ruleId":"149","replacedBy":"150"},{"ruleId":"151","replacedBy":"152"},{"ruleId":"149","replacedBy":"153"},{"ruleId":"151","replacedBy":"154"},{"ruleId":"149","replacedBy":"155"},{"ruleId":"151","replacedBy":"156"},{"ruleId":"157","severity":1,"message":"158","line":33,"column":5,"nodeType":"159","endLine":33,"endColumn":7,"suggestions":"160"},{"ruleId":"161","severity":1,"message":"162","line":8,"column":1,"nodeType":"163","endLine":29,"endColumn":3},{"ruleId":"161","severity":1,"message":"162","line":7,"column":1,"nodeType":"163","endLine":25,"endColumn":3},{"ruleId":"149","replacedBy":"164"},{"ruleId":"151","replacedBy":"165"},{"ruleId":"149","replacedBy":"166"},{"ruleId":"151","replacedBy":"167"},{"ruleId":"157","severity":1,"message":"168","line":51,"column":5,"nodeType":"159","endLine":51,"endColumn":7,"suggestions":"169"},{"ruleId":"157","severity":1,"message":"158","line":17,"column":5,"nodeType":"159","endLine":17,"endColumn":7,"suggestions":"170"},{"ruleId":"157","severity":1,"message":"158","line":19,"column":5,"nodeType":"159","endLine":19,"endColumn":7,"suggestions":"171"},{"ruleId":"157","severity":1,"message":"172","line":40,"column":5,"nodeType":"159","endLine":40,"endColumn":13,"suggestio